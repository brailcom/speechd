complexSymbols:
# identifier	regexp
# Sentence endings.
. sentence ending	(?<=[^\s.])\.(?=[\"'”’)\s]|$)
! sentence ending	(?<=[^\s!])\!(?=[\"'”’)\s]|$)
? sentence ending	(?<=[^\s?])\?(?=[\"'”’)\s]|$)
# Phrase endings.
; phrase ending	(?<=[^\s;]);(?=\s|$)
: phrase ending	(?<=[^\s:]):(?=\s|$)
# Others
decimal point	(?<=\d)\.(?=\d)
in-word '	(?<=[^\W_])['’]
negative number	(?<!\w)-(?=[$£€¥]?\d)

symbols:
# identifier	replacement[[	level][	preserve]][	# display name]

# Complex symbols
. sentence ending	ponc	all	always
! sentence ending	uaillbhreas	all	always
? sentence ending	comhartha ceiste	all	always
; phrase ending	leathstad	most	always
: phrase ending	idirstad	most	always
decimal point		none	always
in-word '	uaschamóg	all	norep
negative number	míneas	none	norep
in-word -	fleiscín	none	always

# Whitespace
\0	blanc	char	# null
\t	tab
\n	fotha líne	char
\f	briseadh idir leathanaigh	none
\r	aisfhilleadh carráiste	char
