import-symbols:
	./import-symbols.py
	./import-orca
	./import-orca _chars

install-data-hook:
	for lang419 in $(ES_419_CLDR) ; do \
		$(MKDIR_P) "$(DESTDIR)$(localedatadir)/$$lang419" ;\
		$(LN_S) "../es_419/emojis.dic" "$(DESTDIR)$(localedatadir)/$$lang419/emojis.dic" || true ;\
	done

uninstall-hook:
	for lang419 in $(ES_419_CLDR) ; do \
		rm "$(DESTDIR)$(localedatadir)/$$lang419/emojis.dic" ;\
	done

