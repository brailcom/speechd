symbols:
# identifier	replacement[[	level][	preserve]][	# display name]

# Complex symbols
. sentence ending	punktum
! sentence ending	utropstegn
? sentence ending	spørsmålstegn
; phrase ending	semikolon
: phrase ending	kolon
in-word '	apostrof
negative number	minus

# Whitespace
\0	blank
\t	tab
\n	ny rad
\f	sidebrytning
\r	radbrytning
 	mellomrom
 	mellomrom # no-break space

# Standard punctuation/symbols
