complexSymbols:
# identifier	regexp
# Sentence endings.
. sentence ending	(?<=[^\s.])\.(?=[\"'”’)\s]|$)
! sentence ending	(?<=[^\s!])\!(?=[\"'”’)\s]|$)
? sentence ending	(?<=[^\s?])\?(?=[\"'”’)\s]|$)
; sentence ending	(?<=[^\s;])\;(?=[\"'”’)\s]|$)
# Phrase endings.
: phrase ending	(?<=[^\s:]):(?=\s|$)
· phrase ending	(?<=[^\s·])·(?=\s|$)
# Others
decimal point	(?<=\d)\.(?=\d)
in-word '	(?<=[^\W_])['’]
negative number	(?<!\w)-(?=\d)

symbols:
# identifier	replacement[[	level][	preserve]][	# display name]

# Complex symbols
. sentence ending	τελεία	all	always
! sentence ending	θαυμαστικό	all	always
? sentence ending	αγγλικό ερωτηματικό	all	always
; sentence ending	ερωτηματικό	all	always
: phrase ending	άνω κάτω τελεία	most	always
· phrase ending	άνω τελεία	most	always
decimal point		none	always
in-word '	απόστροφος	all	norep
negative number	μείον	none	norep

# Whitespace
\0	κενό	char	# null
\t	στηλοθέτης
\n	τέλος γραμμής	char
\f	αλλαγή σελίδας	none
\r	προετοιμασία νέας γραμμής	char
 	διάστημα	char
 	διάστημα	char	# no-break space

