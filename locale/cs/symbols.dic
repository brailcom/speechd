complexSymbols:
# identifier	regexp
# Others
decimal point	(?<=\d)\,(?=\d)

symbols:
# identifier	replacement[[	level][	preserve]][	# display name]

# Complex symbols
. sentence ending	tečka	all	always	# tečka na konci věty
! sentence ending	vykřičník	all	always	# vykřičník na konci věty
? sentence ending	otazník	all	always	# otazník na konci věty
; phrase ending	středník	most	always	# středník uprostřed věty
: phrase ending	dvojtečka	most	always	# dvojtečka uprostřed věty
decimal point		none	always	# desetinná čárka
in-word '	apostrof	all	norep	# apostrof uvnitř slova
negative number	mínus	none	norep	# záporné číslo

# Whitespace
\0	prázdný	char	# znak 0
\t	tab	# tabulátor
\n	nový řádek	char	# line feed
\f	zlom strany	none	# zlom strany
\r	konec řádku	char	# konec řádku
 	mezera	char	# mezera
 	mezera	char	# tvrdá mezera

