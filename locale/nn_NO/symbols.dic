symbols:
# identifier	replacement[[	level][	preserve]][	# display name]

# Complex symbols
. sentence ending	punktum
! sentence ending	ropeteikn
? sentence ending	spørjeteikn
; phrase ending	semikolon
: phrase ending	kolon
in-word '	apostrof

# Whitespace
\0	blank
\t	tab
\n	ny rad
\f	sidebryting
\r	radbryting
 	mellomrom
 	mellomrom # no-break space

